   "source": [
    "import numpy as np\n",
    "import matplotlib.pyplot as plt\n",
    "from mpl_toolkits.mplot3d import Axes3D\n",
    "from ipywidgets import interact, FloatSlider, Layout\n",
    "from IPython.display import display, clear_output\n",
    "\n",
    "# --- Datos Fijos del Problema (Coordenadas de los Puntos) ---\n",
    "# Unidades en pies (ft)\n",
    "punto_A = np.array([0, 0, 0])  # Origen o punto de anclaje\n",
    "punto_B = np.array([21, 0, 11]) # Extremo del cable AB\n",
    "punto_C = np.array([0, 16, 11]) # Extremo del cable AC\n",
    "\n",
    "# Configuración inicial para el gráfico\n",
    "plt.rcParams['figure.figsize'] = [12, 9] # Tamaño del gráfico\n",
    "plt.rcParams['figure.dpi'] = 100 # Resolución del gráfico\n",
    "\n",
    "def analizar_y_graficar_fuerzas(T_AB, T_AC):\n",
    "    \"\"\"\n",
    "    Calcula las fuerzas en el sistema de cables y las visualiza en 3D.\n",
    "    Parámetros:\n",
    "    - T_AB: Tensión en el cable AB (lb)\n",
    "    - T_AC: Tensión en el cable AC (lb)\n",
    "    \"\"\"\n",
    "    clear_output(wait=True) # Limpiar la salida anterior para actualizar el gráfico\n",
    "\n",
    "    # Validaciones básicas\n",
    "    if T_AB < 0 or T_AC < 0:\n",
    "        print(\"Error: Las tensiones deben ser valores no negativos.\")\n",
    "        return\n",
    "\n",
    "    # --- Cálculos Vectoriales ---\n",
    "    vector_AB = punto_B - punto_A\n",
    "    vector_AC = punto_C - punto_A\n",
    "\n",
    "    longitud_AB = np.linalg.norm(vector_AB)\n",
    "    longitud_AC = np.linalg.norm(vector_AC)\n",
    "\n",
    "    u_AB = vector_AB / longitud_AB\n",
    "    u_AC = vector_AC / longitud_AC\n",
    "\n",
    "    F_AB = T_AB * u_AB\n",
    "    F_AC = T_AC * u_AC\n",
    "\n",
    "    F_Resultante = F_AB + F_AC\n",
    "    magnitud_Resultante = np.linalg.norm(F_Resultante)\n",
    "\n",
    "    # --- Presentación de Resultados (Numéricos) ---\n",
    "    print(\"============================================================\")\n",
    "    print(\"RESULTADOS DEL ANÁLISIS ESTRUCTURAL\")\n",
    "    print(\"============================================================\")\n",
    "    print(f\"Cable AB:\")\n",
    "    print(f\"  - Longitud: {longitud_AB:.2f} ft\")\n",
    "    print(f\"  - Tensión: {T_AB} lb\")\n",
    "    print(f\"  - Vector de Fuerza (Fx, Fy, Fz): [{F_AB[0]:.1f}, {F_AB[1]:.1f}, {F_AB[2]:.1f}] lb\")\n",
    "    print(f\"\nCable AC:\")\n",
    "    print(f\"  - Longitud: {longitud_AC:.2f} ft\") \n",
    "    print(f\"  - Tensión: {T_AC} lb\")\n",
    "    print(f\"  - Vector de Fuerza (Fx, Fy, Fz): [{F_AC[0]:.1f}, {F_AC[1]:.1f}, {F_AC[2]:.1f}] lb\")\n",
    "    print(f\"\nFuerza Resultante:\")\n",
    "    print(f\"  - Magnitud: {magnitud_Resultante:.1f} lb\")\n",
    "    print(f\"  - Vector Resultante (Rx, Ry, Rz): [{F_Resultante[0]:.1f}, {F_Resultante[1]:.1f}, {F_Resultante[2]:.1f}] lb\")\n",
    "    print(\"============================================================\")\n",
    "\n",
    "    # --- Visualización Gráfica 3D ---\n",
    "    fig = plt.figure(figsize=(12, 9))\n",
    "    ax = fig.add_subplot(111, projection='3d')\n",
    "\n",
    "    # Dibujar los puntos A, B, C\n",
    "    ax.scatter(*punto_A, color='red', s=100, label='Punto A (Anclaje)')\n",
    "    ax.text(*punto_A, ' A', fontsize=12, fontweight='bold', color='red')\n",
    "    ax.scatter(*punto_B, color='blue', s=100)\n",
    "    ax.text(*punto_B, ' B', fontsize=12, fontweight='bold', color='blue')\n",
    "    ax.scatter(*punto_C, color='green', s=100)\n",
    "    ax.text(*punto_C, ' C', fontsize=12, fontweight='bold', color='green')\n",
    "\n",
    "    # Dibujar los cables (líneas de acción)\n",
    "    ax.plot([punto_A[0], punto_B[0]], [punto_A[1], punto_B[1]], [punto_A[2], punto_B[2]], 'b--', linewidth=2, label=f'Cable AB ({T_AB} lb)')\n",
    "    ax.plot([punto_A[0], punto_C[0]], [punto_A[1], punto_C[1]], [punto_A[2], punto_C[2]], 'g--', linewidth=2, label=f'Cable AC ({T_AC} lb)')\n",
    "\n",
    "    # Dibujar los vectores de fuerza (usando quiver)\n",
    "    # Se escalan los vectores para una mejor visualización en la gráfica\n",
    "    escala = 0.015\n",
    "    ax.quiver(*punto_A, *(F_AB * escala), color='blue', linewidth=2, arrow_length_ratio=0.1, label='Fuerza AB')\n",
    "    ax.quiver(*punto_A, *(F_AC * escala), color='green', linewidth=2, arrow_length_ratio=0.1, label='Fuerza AC')\n",
    "    ax.quiver(*punto_A, *(F_Resultante * escala), color='red', linewidth=3, arrow_length_ratio=0.1, label=f'Fuerza Resultante ({magnitud_Resultante:.0f} lb)')\n",
    "\n",
    "    # --- Configuración de la Gráfica ---\n",
    "    ax.set_xlabel('Eje X (ft)', fontweight='bold')\n",
    "    ax.set_ylabel('Eje Y (ft)', fontweight='bold')\n",
    "    ax.set_zlabel('Eje Z (ft)', fontweight='bold')\n",
    "    ax.set_title('Análisis de Fuerzas en Sistema de Cables 3D', fontsize=16, fontweight='bold')\n",
    "    ax.legend()\n",
    "    ax.grid(True)\n",
    "\n",
    "    # Ajustar los límites de los ejes para una mejor perspectiva\n",
    "    ax.set_xlim([0, 25])\n",
    "    ax.set_ylim([0, 20])\n",
    "    ax.set_zlim([0, 15])\n",
    "\n",
    "    plt.show()\n",
    "\n",
    "# --- Creación de los Sliders Interactivos ---\n",
    "# Se definen los rangos y valores iniciales para cada parámetro.\n",
    "interact(analizar_y_graficar_fuerzas,\n",
    "         T_AB=FloatSlider(min=100, max=2000, step=10, value=840, description='Tensión AB (lb):', layout=Layout(width='auto')),\n",
    "         T_AC=FloatSlider(min=100, max=2000, step=10, value=1200, description='Tensión AC (lb):', layout=Layout(width='auto'))\n",
    "        );"
   ]
  }
